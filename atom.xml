---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>OS Blog</title>
  <link href="http://rs.io"/>
  <link type="application/atom+xml" rel="self" href="http://rs.io/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://os-blog.com</id>
  <author>
    <name>Robert Seaton</name>
    <email>seatonr@dupage.edu</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://rs.io{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://os-blog.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Robert Seaton</name>
      <uri>http://os-blog.com</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
